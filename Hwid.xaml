<Window x:Class="CSNPS.Hwid"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSNPS"
        mc:Ignorable="d"
        WindowStyle="None"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        Title="hwid" Height="160" Width="420">
    <Window.Background>
        <ImageBrush ImageSource="/hwid_backgroud.png"/>
    </Window.Background>
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/hwid_backgroud.png"/>
        </Grid.Background>
        <TextBlock x:Name="hwidtxt" HorizontalAlignment="Center" Margin="0,141,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="420" Height="19" Foreground="White" FontSize="11"/>

    </Grid>
</Window>
